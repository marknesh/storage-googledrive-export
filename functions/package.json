{
  "name": "functions",
  "scripts": {
    "lint": "eslint \"src/**/*\" \"__tests__/**/*\"",
    "lint:fix": "eslint \"src/**/*\" --fix",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "tsc && cd __tests__ && tsc && cd ../firebase-config && firebase emulators:exec 'jest --detectOpenHandles' --project demo-test",
    "dev": "cd firebase-config && npm run build:watch | firebase emulators:start --project demo-test",
    "prettier": "prettier --write .",
    "prepare": "cd .. && husky install"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": "please-use-npm"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^1.8.2",
    "dotenv": "^16.3.1",
    "firebase-admin": "^13.1.0",
    "firebase-functions": "^6.1.1",
    "googleapis": "^128.0.0",
    "husky": "^8.0.3",
    "typescript": "^4.9.0"
  },
  "overrides": {
    "firebase-functions-test": {
      "firebase-admin": "^13.1.0"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.5.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.7",
    "eslint": "^8.15.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.26.0",
    "firebase-functions-test": "^3.4.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0"
  },
  "private": true
}
